        particule5.y=-50;
        arduboy.clear();
        arduboy.setCursor(0,0);
        arduboy.print("Level 4 !");
        Sprites::drawSelfMasked(50,12,thunder,0);
        arduboy.display();
        delay(5000);
        level++;
      } else if (count==2000)
      {
        particule1.y=-5;
        particule2.y=-10;
        particule3.y=-15;
        particule4.y=-20;
        particule5.y=-25;
        arduboy.clear();
        arduboy.setCursor(0,0);
        arduboy.print("Last Level : 5 !");
        Sprites::drawSelfMasked(50,12,thunder,0);
        arduboy.display();
        delay(5000);
        level++;
      }
      //If two particles are in the circle, then DIE ! (yes, i do love optimization)
      if(particule1.y==32-5 and particule2.y==32-5 or particule1.y==32-5 and particule3.y==32-5 or particule1.y==32-5 and particule4.y==32-5 or particule1.y==32-5 and particule5.y==32-5 or particule2.y==32-5 and particule3.y==32-5 or particule2.y==32-5 and particule4.y==32-5 or particule2.y==32-5 and particule5.y==32-5 or particule3.y==32-5 and particule4.y==32-5 or particule3.y==32-5 and particule5.y==32-5 or particule4.y==32-5 and particule5.y==32-5){
        gamestatus=2;
        break;
      }
      
      //Verif of particule1
      if (particule1.Verif()==-1){
        gamestatus=2;
        break;
      }
      //Verif of particule2
      if (particule2.Verif()==-1){
        gamestatus=2;
        break;
      }
      //Verif of particule3
      if (particule3.Verif()==-1){
        gamestatus=2;
        break;
      }
      //Verif of particule4
      if (particule4.Verif()==-1){
        gamestatus=2;
        break;
      }
      //Verif of particule5
      if (particule5.Verif()==-1){
        gamestatus=2;
        break;
      }
      
      arduboy.display();
      break;

    case 2: //Take care of the Game Over Screen
      arduboy.clear();
      arduboy.setCursor(1,0);
      arduboy.print("Game over !");
      gameover.Draw(GameOverdecor);
      arduboy.setCursor(1,20);
      arduboy.print("Score :");
      arduboy.setCursor(30,20);
      arduboy.print(count);
      arduboy.setCursor(1,30);
      arduboy.print("Level :");
      arduboy.setCursor(50,30);
      arduboy.print(level);
      arduboy.setCursor(1,40);
      arduboy.print("A button to title screen");
      arduboy.setCursor(1,50);
      arduboy.print("B button to retry");
      arduboy.display();
      if(arduboy.pressed(A_BUTTON) and justpressed == 0)
      {
        justpressed=1;
        gamestatus=0;
      }
      if(arduboy.pressed(B_BUTTON) and justpressed==0)
      {
        justpressed=1;
        gamestatus=1;
      }
      break;
    }
  
  if(arduboy.notPressed(A_BUTTON))
  {
    justpressed = 0;
  }
  arduboy.display();
}
